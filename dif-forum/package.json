{
  "name": "dif-forum",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"pnpm run start:11ty\" \"pnpm run start:css\"",
    "start:11ty": "pnpm exec eleventy --serve",
    "start:css": "postcss ./src/styles/main.css -o ./_site/styles.css --watch",
    "build": "set NODE_ENV=production && pnpm run clean && pnpm run build:css && pnpm run build:11ty",
    "build:11ty": "pnpm exec eleventy --pathprefix /forum/",
    "build:css": "postcss ./src/styles/main.css -o ./_site/styles.css",
    "clean": "rimraf ./_site",
    "debug": "set DEBUG=Eleventy* && pnpm exec eleventy --serve",
    "deploy": "pnpm run build && pnpm run deploy:gh",
    "deploy:gh": "gh-pages --dist _site --message Deploy --no-history"
  },
  "keywords": [],
  "author": "iwrite@selfdocumentingco.de",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "~2.0.1",
    "@grimlink/eleventy-plugin-lucide-icons": "~1.0.4",
    "@shopify/prettier-plugin-liquid": "~1.3.2",
    "@tailwindcss/typography": "~0.5.10",
    "autoprefixer": "~10.4.16",
    "concurrently": "~8.2.1",
    "cssnano": "~6.0.1",
    "daisyui": "~3.9.2",
    "debug": "~4.3.4",
    "dotenv": "~16.3.1",
    "eslint": "~8.51.0",
    "eslint-config-prettier": "~9.0.0",
    "gh-pages": "~6.0.0",
    "postcss": "~8.4.31",
    "postcss-cli": "~10.1.0",
    "postgres": "~3.4.0",
    "prettier": "~3.0.3",
    "prettier-plugin-tailwindcss": "~0.5.5",
    "rimraf": "~5.0.5",
    "tailwindcss": "~3.3.3"
  },
  "packageManager": "pnpm@8.14.1+sha256.2df78e65d433d7693b9d3fbdaf431b2d96bb4f96a2ffecd51a50efe16e50a6a8"
}
